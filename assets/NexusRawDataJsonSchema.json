{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://nexus-archive.zerozero.pl/submit-json",
  "type": "object",
  "properties": {
    "sessionId": {
      "type": "string"
    },
    "requestId": {
      "type": "string"
    },
    "previousRequestId": {
      "type": [
        "string",
        "null"
      ]
    },
    "requestStartedAt": {
      "type": "string",
      "format": "date-time"
    },
    "responseCompletedAt": {
      "type": "string",
      "format": "date-time"
    },
    "method": {
      "type": "string",
      "enum": [
        "GET",
        "POST"
      ]
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "formData": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": true
    },
    "responseBody": {
      "type": "string"
    }
  },
  "required": [
    "sessionId",
    "requestId",
    "previousRequestId",
    "requestStartedAt",
    "responseCompletedAt",
    "method",
    "url",
    "formData",
    "responseBody"
  ],
  "additionalProperties": false
}
